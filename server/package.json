{
  "name": "sleepyhead",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy-swagger-config": "cp ./src/config/*.yaml ./dist/config",
    "csc": "npm run copy-swagger-config",
    "postinstall": "npx tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "start": "node ./dist/server.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "REST API Server for Sleepyhead",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^5.0.0-alpha.7",
    "pm2": "^3.5.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "supertest": "^3.4.2",
    "swagger-ui-express": "^4.1.2",
    "tsc-watch": "^1.1.39",
    "typescript": "^3.6.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.12.12",
    "@types/request-promise": "^4.1.42",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/yamljs": "^0.2.30",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5"
  }
}
